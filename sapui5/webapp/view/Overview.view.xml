<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" 
          controllerName="logaligroup.sapui5.controller.App"
          displayBlock="true" xmlns:core="sap.ui.core">

            <Page id="_IDGenPage1" title="{i18n>appTitle}">
                <landmarkInfo>
                    <PageAccessibleLandmarkInfo rootRole="Region"
                                                rootLabel="{i18n>overviewRootLabel}"
                                                contentRole="Main"
                                                contentLabel="{i18n>overviewContentLabel}"
                                                headerRole="Banner"
                                                headerLabel="{i18n>overviewHeaderLabel}">

                    </PageAccessibleLandmarkInfo>
                </landmarkInfo>
                <headerContent>
                <Button id="btn_hdialogHeader" 
                        icon="sap-icon://action"
                        press=".onOpenDialogHeader" 
                        class="sapUiSmallMarginEnd" />
                </headerContent>
                <content>
                        <mvc:XMLView id="_IDGenXMLView1" viewName="logaligroup.sapui5.view.HelloPanel"/>
                        <mvc:XMLView id="_IDGenXMLView2" viewName="logaligroup.sapui5.view.InvoicesList"/>
                    </content>
                </Page>
</mvc:View>